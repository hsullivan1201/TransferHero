# WMATA API Key (required)
WMATA_API_KEY=your_api_key_here

# Server port (optional, default 3001)
PORT=3001

# Environment (optional, default development)
NODE_ENV=development

# CORS origin (optional, default *)
CORS_ORIGIN=http://localhost:5173

# GTFS refresh cron schedule (optional, default "0 3 * * *" = 3 AM daily)
GTFS_REFRESH_CRON=0 3 * * *
